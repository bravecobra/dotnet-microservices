{
  "service": {
    "name": "deliveries",
    "id":"deliveries-v1",
    "port": 8002,
    "address":"10.0.0.4",
    "tags":[
      "deliveries",
      "urlprefix-/deliveries strip=/deliveries"
    ],
    "checks": [
      {
        "id": "api",
        "name": "HTTP API on port 8002",
        "http": "http://10.0.0.4:8002/health",
        "interval": "10s"
      }
    ],
    "connect": {
      "sidecar_service": {
        "port": 20000,
        "checks": [
          {
          "name": "Connect Envoy Sidecar",
          "tcp": "10.0.0.4:20000",
          "interval": "10s"
          },
          {
          "name": "Connect Sidecar Aliasing deliveries",
          "alias_service": "deliveries"
          }
        ],
        "tags":[
          "deliveries"
        ]
      }
    }
  }
}
